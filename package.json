{
  "name": "quickcredit",
  "version": "1.0.0",
  "description": "Online lending platform",
  "main": "app.js",
  "scripts": {
    "test": "nyc mocha ./server/test/**/*.test.js --exec babel-node --require babel-core/register --timeout 10000 --exit",
    "start-test": "babel-node ./server/app.js  --exec babel-node",
    "start:server": "babel-node -- ./server/app.js",
    "dev": "nodemon --exec babel-node ./server/app.js",
    "build-test": "babel ./server/app.js --out-dir build",
    "coverage": "nyc report --reporter=lcov --reporter=text | coveralls",
    "start": "node ./build/app.js",
    "build": "babel server/ --out-dir build/",
    "create-tables": "babel-node ./server/db/createTables.js" 
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/olumidae/QuickCredit.git"
  },
  "keywords": [
    "quickcredit"
  ],
  "author": "Omitiran Olumide",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/olumidae/QuickCredit/issues"
  },
  "homepage": "https://github.com/olumidae/QuickCredit#readme",
  "dependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-preset-env": "^1.7.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-register": "^6.26.0",
    "bcrypt": "^3.0.6",
    "body-parser": "^1.19.0",
    "chai": "^4.2.0",
    "chai-http": "^4.3.0",
    "debug": "^4.1.1",
    "dotenv": "^8.0.0",
    "express": "^4.16.4",
    "joi": "^14.3.1",
    "jsonwebtoken": "^8.5.1",
    "moment": "^2.24.0",
    "nyc": "^14.1.1",
    "pg": "^7.11.0"
  },
  "devDependencies": {
    "babel-plugin-transform-runtime": "^6.23.0",
    "coveralls": "^3.0.3",
    "eslint": "^5.16.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-plugin-import": "^2.17.2",
    "mocha": "^6.1.4",
    "mocha-lcov-reporter": "^1.3.0",
    "morgan": "^1.9.1",
    "nodemon": "^1.19.0"
  }
}
